{
  "name": "movie_service",
  "version": "1.0.0",
  "description": "movie_service for movie_ticket_booking",
  "main": "main.ts",
  "scripts": {
    "api-knex": "TS_NODE_COMPILER_OPTIONS='{ \"module\": \"commonjs\" }' knex --knexfile=./knexfile.ts",
    "clean": "rm -r ./src/proto/gen && tsc --build --clean",
    "build": "npm run generate && tsc --build",
    "watch-grpc": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node -- --start_grpc_server\"",
    "watch-kafka": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node -- --start_kafka_consumer\"",
    "watch-node": "nodemon --ignore \"logs/**\" --delay 1s --inspect dist/main.js",
    "debug-grpc": "npm run build && npm run watch-grpc",
    "debug-kafka": "npm run build && npm run watch-kafka",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-ts": "tsc -w",
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "generate": "./node_modules/.bin/proto-loader-gen-types --longs=Number --enums=Number --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto/gen src/proto/**/*.proto",
    "migration": "knex migrate:latest",
    "make": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "repair": "knex migrate:repair",
    "serve-grpc": "ts-node src/main.ts --start_grpc_server",
    "serve": "node dist/main.js",
    "start-initialize": "node dist/main.js --prod --initialize",
    "start-grpc": "node dist/main.js --prod --start_grpc_server"
  },
  "keywords": [],
  "author": "halerquinz",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.12.2",
    "@types/knex": "^0.16.1",
    "brandi": "^5.0.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "kafkajs": "^2.2.4",
    "knex": "^3.1.0",
    "minimist": "^1.2.8",
    "minio": "^7.1.3",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "nodejs-snowflake": "^2.0.1",
    "nodemon": "^3.1.0",
    "pg": "^8.11.3",
    "sharp": "^0.33.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "validator": "^13.11.0",
    "winston": "3.6.0",
    "winston-daily-rotate-file": "^4.6.1",
    "winston-elasticsearch": "^0.18.0",
    "xss": "^1.0.15"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.5.5",
    "@grpc/proto-loader": "^0.6.9",
    "@types/minimist": "^1.2.5",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.25",
    "@types/validator": "^13.11.9",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0"
  }
}
