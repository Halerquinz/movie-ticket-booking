{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-watch": "nodemon dist/main.js",
    "serve": "node dist/main.js",
    "start": "node dist/main.js --prod",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon --ignore \"logs/**\" --delay 1s --inspect dist/main.js",
    "lint": "npx eslint ./src --ext .js,.jsx,.ts,.tsx",
    "generate": "./node_modules/.bin/proto-loader-gen-types --longs=Number --enums=Number --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto/gen src/proto/**/*.proto",
    "migration": "knex migrate:latest",
    "make": "knex migrate:make",
    "rollback": "knex migrate:rollback",
    "repair": "knex migrate:repair"
  },
  "keywords": [],
  "author": "halerquinz",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^8.12.2",
    "@zapier/secret-scrubber": "^1.1.0",
    "brandi": "^5.0.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "http-status": "^1.7.4",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.0",
    "typescript": "^5.3.3",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^5.0.0",
    "winston-elasticsearch": "^0.18.0"
  },
  "devDependencies": {
    "@grpc/grpc-js": "^1.10.2",
    "@grpc/proto-loader": "^0.7.10",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0"
  }
}
